{
  "buildCommand": "turbo run build",
  "installCommand": "npm install",
  "outputDirectory": "apps/frontend/dist",
  "functions": {
    "api/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/backend/dist/main.js"
    },
    {
      "src": "/(.*)",
      "dest": "apps/frontend/dist/index.html"
    }
  ],
  "framework": "vite"
}
