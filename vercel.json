{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "apps/frontend/dist",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/frontend/dist/$1"
    }
  ]
}
