{
  "version": 2,
  "buildCommand": "turbo run build",
  "devCommand": "turbo run dev",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": ".",
  "builds": [
    {
      "src": "apps/backend/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/frontend/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/backend/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "apps/frontend/dist/$1"
    }
  ]
}
