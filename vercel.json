{
  "buildCommand": "turbo run build --filter=frontend^...",
  "installCommand": "npm install",
  "outputDirectory": "apps/frontend/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "framework": "vite"
}
